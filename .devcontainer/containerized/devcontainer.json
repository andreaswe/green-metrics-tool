{
    "image": "ghcr.io/green-coding-berlin/codespace-container:latest",
    "onCreateCommand": "/workspaces/green-metrics-tool/.devcontainer/containerized/codespace-setup.sh",
    "postStartCommand": "/workspaces/green-metrics-tool/.devcontainer/on-start.sh",
    "forwardPorts": [9143],
    "portsAttributes": {
        "9143": {
            "label": "metrics page"
        },
        "9142": {
            "label": "api"
        }
    },
    "customizations": {
        "codespaces": {
            "openFiles": [
                ".devcontainer/splash.md"
            ]
        }
    }
}
